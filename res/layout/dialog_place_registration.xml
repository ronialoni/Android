<?xml version="1.0" encoding="UTF-8"?>
    <TableLayout 
    	android:id				="@+id/DPRtableLayout1" 
    	android:layout_width	="fill_parent" 
    	android:layout_height	="fill_parent" 
		    android:background = "@drawable/background"
    	xmlns:android			="http://schemas.android.com/apk/res/android">
        
        <TextView 
        	android:text="Title" 
        	android:id="@+id/DPRTitle" 
        	android:layout_height="wrap_content"
        	android:gravity="center"
	    	android:layout_width="fill_parent"
		    android:textStyle = "bold"
		    android:typeface  = "serif"
		    android:textSize = "20dp"
		    android:background = "@drawable/background"
        	/>
        
        <TextView 
        	android:text			="Address:" 
        	android:layout_height	="wrap_content" 
        	android:layout_width	="wrap_content" 
            android:textColor 		= "#ffffff"
            android:typeface		= "serif"
        	android:id				="@+id/DPRaddressTitle"></TextView>
        	 
        <TextView 
        	android:text			="//TODO:  Address of place" 
        	android:layout_height	="wrap_content" 
        	android:layout_width	="wrap_content" 
        	android:id				="@+id/DPRaddress"></TextView>
        
        <TableRow 
        	android:id				="@+id/DPRtableRowOpenTitles" 
        	android:layout_width	="fill_parent" 
        	android:layout_height	="wrap_content"
        	android:orientation		="horizontal"
        	>
        
	        <TextView 
	        	android:text			="Opens From:" 
	        	android:layout_height	="wrap_content" 
	        	android:layout_width	="wrap_content" 
	            android:textColor 		= "#ffffff"
	            android:typeface		= "serif"
	            android:layout_weight="0.5"
	        	android:id				="@+id/DPRdatesTitleFrom"></TextView>
	        	
	        <TextView 
	        	android:text			="Opens until:" 
	        	android:layout_height	="wrap_content" 
	        	android:layout_width	="wrap_content" 
	            android:textColor 		= "#ffffff"
	            android:layout_weight="0.5"
	            android:typeface		= "serif"
	        	android:id				="@+id/DPRdatesTitleUntil"></TextView>
	        	
	        </TableRow>
        <TableRow 
     	  	android:id				="@+id/DPRtableRowOpenValues" 
       		android:layout_width	="fill_parent" 
       		android:layout_height	="wrap_content"
       		android:orientation		="horizontal"
       	>
	        
	        <TextView 
	        	android:text			="//TODO:  Dates of place" 
	        	android:layout_height	="wrap_content"
	        	android:layout_weight="0.5" 
	        	android:layout_width	="wrap_content" 
	        	android:id				="@+id/DPRdatesFrom"></TextView>
	        	
        	<TextView 
	        	android:text			="//TODO:  Dates of place"
	        	android:layout_weight="0.5" 
	        	android:layout_height	="wrap_content" 
	        	android:layout_width	="wrap_content" 
	        	android:id				="@+id/DPRdatesUntil"></TextView>
	        	
	      </TableRow>
	        	
        
        <TableRow 
        	android:id				="@+id/DPRtableRow12" 
        	android:layout_width	="fill_parent" 
        	android:layout_height	="wrap_content">
            
            <TextView 
            	android:text			="Prayers:" 
            	android:layout_height	="wrap_content" 
            	android:layout_width	="fill_parent" 
           		android:typeface		= "serif"
            	android:textColor 		= "#ffffff"
            	android:layout_weight	= "1"
            	android:id				="@+id/DPRselectPrayers"></TextView>

            <TextView 
            	android:text			="Time:" 
            	android:layout_height	="wrap_content" 
            	android:layout_width	="fill_parent" 
           		android:typeface		= "serif"
            	android:textColor 		= "#ffffff"
            	android:layout_weight	= "1"
            	android:id				="@+id/DPRselectPrayers"></TextView>
            
            <TextView 
            	android:text			="Users:" 
            	android:layout_height	="wrap_content" 
            	android:layout_width	="fill_parent" 
           		android:typeface		= "serif"
            	android:textColor 		= "#ffffff"
            	android:id				="@+id/DPRusersNumber"></TextView>
            
            <TextView 
            	android:layout_height	="wrap_content" 
            	android:layout_width	="fill_parent" 
            	android:id				="@+id/DPRselectPrayers"></TextView>
            
        </TableRow>
        
        <TableRow 
        	android:id				="@+id/DPRtableRow13" 
        	android:layout_width	="fill_parent" 
        	android:layout_height	="wrap_content">
            
            <CheckBox 
            	android:layout_height	="wrap_content" 
            	android:layout_width	="wrap_content" 
            	android:layout_weight	= "1"
            	android:clickable		= "false"
            	android:textColor		= "#808080"
            	android:id				="@+id/DPRcheckboxShaharit"
            	android:text			="Shaharit"></CheckBox>
            
            <TextView 
            	android:text			="00:00" 
            	android:id				="@+id/DPRtimeShaharit" 
            	android:layout_width	="wrap_content" 
            	android:layout_weight	= "1"
            	android:textColor		= "#808080"
            	android:gravity			="center_vertical" 
            	android:layout_height	="wrap_content"></TextView>
            	
            <Button 
            	android:text			="0" 
            	android:gravity			="center" 
            	android:layout_height	="wrap_content"
            	android:textColor		= "#808080" 
            	android:layout_width	="fill_parent" 
            	android:id				="@+id/DPRnumberOfUsersShaharit"></Button>
            
            <ImageView 
            	android:layout_width	="fill_parent" 
            	android:layout_height	="wrap_content"
            	android:src				="@drawable/shaharit_small" 
            	android:layout_gravity	="right" 
            	android:id				="@+id/DPRiconShaharit"></ImageView>
        </TableRow>
       
        <TableRow 
        	android:id				="@+id/DPRtableRow23" 
        	android:layout_width	="wrap_content" 
        	android:layout_height	="wrap_content">
            
            <CheckBox 
            	android:layout_height	="wrap_content" 
            	android:layout_width	="wrap_content" 
            	android:layout_weight 	= "1"
            	android:clickable		= "false"
            	android:textColor		= "#808080"
            	android:id				="@+id/DPRcheckboxMinha" 
            	android:text			="Minha"></CheckBox>
            
             <TextView 
            	android:text			="00:00" 
            	android:id				="@+id/DPRtimeMinha" 
            	android:layout_width	="wrap_content" 
            	android:layout_weight	= "1"
            	android:textColor		= "#808080"
            	android:gravity			="center_vertical" 
            	android:layout_height	="wrap_content" 
            	></TextView>
            
            <Button 
            	android:text			="0" 
            	android:layout_height	="wrap_content" 
            	android:layout_width	="wrap_content" 
            	android:textColor		= "#808080"
            	android:id				="@+id/DPRnumberOfUsersMinha" 
            	android:gravity			="center"></Button>
            
            <ImageView 
            	android:layout_width	="wrap_content" 
            	android:layout_height	="wrap_content" 
            	android:src				="@drawable/minha_small" 
            	android:id				="@+id/DPRiconMinha"></ImageView>
        </TableRow>
        
        <TableRow 
        	android:id				="@+id/DPRtableRow33" 
        	android:layout_width	="wrap_content" 
        	android:layout_height	="wrap_content">
            
            <CheckBox 
            	android:layout_height	="wrap_content" 
            	android:layout_width	="wrap_content" 
            	android:layout_weight 	= "1"
            	android:clickable		= "false"
            	android:textColor		= "#808080"
            	android:id				="@+id/DPRcheckboxArvit" 
            	android:text			="Arvit"></CheckBox>

 			<TextView 
            	android:text			="00:00" 
            	android:id				="@+id/DPRtimeArvit" 
            	android:layout_width	="wrap_content" 
            	android:layout_weight	= "1"
            	android:textColor		= "#808080"
            	android:gravity			="center_vertical" 
            	android:layout_height	="wrap_content"></TextView>
                        
            <Button 
            	android:text			="0" 
            	android:layout_height	="wrap_content" 
            	android:layout_width	="wrap_content"
            	android:textColor		= "#808080" 
            	android:id				="@+id/DPRnumberOfUsersArvit" 
            	android:gravity			="center"></Button>
            
            <ImageView 
            	android:layout_width	="wrap_content" 
            	android:layout_height	="wrap_content" 
            	android:src				="@drawable/arvit_small" 
            	android:id				="@+id/DPRiconArvit"></ImageView>
        </TableRow>
        
        <LinearLayout 
            android:id="@+id/DPRlinearLayout11" 
            android:layout_width="wrap_content" 
            android:layout_height="wrap_content">
            
           <Button 
               android:text="Set" 
               android:id="@+id/DPRSetButton" 
               android:layout_height="wrap_content" 
               android:layout_width="fill_parent"
               android:layout_weight = ".2"
            ></Button>
            <Button 
               android:text="Close" 
               android:layout_height="wrap_content" 
               android:id="@+id/DPRCloseButton" 
               android:layout_width="fill_parent"
               android:layout_weight = ".2"
               />
        </LinearLayout>
        <Button android:id				="@+id/DPRDeleteButton"  
        		android:text			="Delete Place" 
        		android:layout_width	="fill_parent" 
        		android:layout_height	="wrap_content"
        		></Button>
    </TableLayout>
